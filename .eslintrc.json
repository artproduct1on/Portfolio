{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "@next/next/no-img-element": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "no-unused-vars": "off",
    "no-undef": "error",
    "semi": [
      "error",
      "always"
    ],
    "indent": [
      "warn",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-var": "error",
    "quotes": [
      "warn",
      "double"
    ],
    "eol-last": [
      "warn",
      "always"
    ],
    "no-plusplus": "off",
    "comma-dangle": "off",
    "prefer-destructuring": "off",
    "no-param-reassign": "off",
    "max-len": [
      "warn",
      150,
      {
        "ignoreUrls": true
      }
    ],
    "object-curly-spacing": [
      "warn",
      "always"
    ],
    "array-bracket-spacing": [
      "warn",
      "never"
    ],
    "space-before-blocks": [
      "warn",
      "always"
    ],
    "arrow-spacing": [
      "warn",
      {
        "before": true,
        "after": true
      }
    ],
    "keyword-spacing": [
      "warn",
      {
        "before": true,
        "after": true
      }
    ],
    "space-infix-ops": "warn",
    "spaced-comment": [
      "warn",
      "always"
    ],
    "comma-spacing": [
      "warn",
      {
        "before": false,
        "after": true
      }
    ],
    "func-call-spacing": [
      "warn",
      "never"
    ],
    "no-multiple-empty-lines": [
      "warn",
      {
        "max": 1
      }
    ],
    "object-curly-newline": [
      "error",
      {
        "ImportDeclaration": {
          "multiline": true,
          "consistent": true
        }
      }
    ],
    "react/react-in-jsx-scope": "off"
  }
}